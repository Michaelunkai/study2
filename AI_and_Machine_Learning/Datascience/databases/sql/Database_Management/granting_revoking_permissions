GRANT DELETE ON mydatabase.mytable TO 'user4'@'localhost';
GRANT CREATE ON database_name TO 'username'@'localhost';
 
 
GRANT CREATE ON mydatabase TO 'user5'@'localhost';
GRANT DROP ON database_name TO 'username'@'localhost';
 
 
GRANT DROP ON mydatabase TO 'user6'@'localhost';
GRANT ALTER ON database_name.table_name TO 'username'@'localhost';
 
 
GRANT ALTER ON mydatabase.mytable TO 'user7'@'localhost';
GRANT INDEX ON database_name.table_name TO 'username'@'localhost';
 
 
GRANT INDEX ON mydatabase.mytable TO 'user8'@'localhost';
GRANT REFERENCES ON database_name.table_name TO 'username'@'localhost';
 
 
GRANT REFERENCES ON mydatabase.mytable TO 'user9'@'localhost';
GRANT EXECUTE ON procedure_name TO 'username'@'localhost';
 
 
GRANT EXECUTE ON myprocedure TO 'user10'@'localhost';
GRANT ALL PRIVILEGES ON database_name.table_name TO 'username'@'localhost';
 
 
GRANT ALL PRIVILEGES ON mydatabase.mytable TO 'user11'@'localhost';
