git clone https://github.com/MarquezProject/marquez.git && cd marquez && ./docker/up.sh && curl http://localhost:5000 && pip install openlineage-python && mkdir -p ~/.openlineage && echo -e "transport:\n  type: http\n  url: http://localhost:5000" > ~/.openlineage/openlineage.yml && curl -X POST http://localhost:5000/api/v1/lineage -H 'Content-Type: application/json' -d '{"eventType":"START","eventTime":"2024-11-29T01:04:16.000+02:00","run":{"runId":"unique-run-id"},"job":{"namespace":"default","name":"example-job"},"inputs":[],"producer":"https://github.com/OpenLineage/OpenLineage"}' && gc http://localhost:3000
