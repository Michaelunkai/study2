sudo apt-get install -y qemu-user-static && docker run --rm --privileged multiarch/qemu-user-static --reset -p yes && mkdir -p ~/rpi-os && cd ~/rpi-os && printf 'FROM balenalib/rpi-raspbian:latest\nCMD ["bash"]' > Dockerfile && docker build -t rpi-os . && docker run -it --platform linux/arm/v6 rpi-os
